<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_weight="1"
    tools:layout_editor_absoluteX="47dp"
    tools:layout_editor_absoluteY="104dp">

    <ImageView
        android:id="@+id/logo_circle3"
        android:layout_width="20dp"
        android:layout_height="18dp"
        android:layout_centerInParent="true"
        android:paddingTop="1.5dp"
        app:srcCompat="@drawable/sglab_logo_circle_3"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="108dp"
        tools:ignore="ContentDescription"/>

    <ImageView
        android:id="@+id/logo_circle2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_centerInParent="true"
        app:srcCompat="@drawable/sglab_logo_circle_2"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="103dp"
        tools:ignore="ContentDescription"/>

    <ImageView
        android:id="@+id/logo_circle1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:adjustViewBounds="false"
        app:srcCompat="@drawable/sglab_logo_circle_1"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="99dp"
        tools:ignore="ContentDescription"/>
</RelativeLayout>